{
  "name": "protobufjs",
  "version": "3.0.0",
  "description": "Protocol Buffers for JavaScript. Finally.",
  "author": {
    "name": "Daniel Wirtz",
    "email": "dcode@dcode.io"
  },
  "contributors": [
    {
      "name": "Frank Xu",
      "email": "yyfrankyy@gmail.com"
    }
